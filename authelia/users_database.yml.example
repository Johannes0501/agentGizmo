---
# Authelia Users Database Template
#
# Copy this file to users_database.yml and update with your details
#
# To generate a password hash, run:
#   docker run --rm authelia/authelia:latest authelia crypto hash generate argon2 --password 'YOUR_PASSWORD_HERE'

users:
  your_username:
    displayname: "Your Name"
    password: "$argon2id$v=19$m=65536,t=3,p=4$REPLACE_WITH_YOUR_HASH"
    email: your@email.com
    groups:
      - admins
